{
    "name": "npdo",
    "version": "0.0.5",
    "description": "Pdo for Node",
    "author": "Claudio Pennati <claudio.pennati@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lupennat/npdo.git"
    },
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist/*",
        "@types/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepublishOnly": "safe-publish-latest && npm run lint && npm run test && npm run build",
        "prepublish": "not-in-publish || npm run prepublishOnly",
        "clean": "del-cli dist",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "compile": "npm run clean && tsc",
        "build": "npm run compile",
        "watch": "tsc -w",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "engines": {
        "node": "^14.19.0 || ^16.10.0 || >=18.0.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/jest": "^29.1.2",
        "@types/lodash.shuffle": "^4.2.7",
        "@types/node": "^16.18.0",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "del-cli": "^5.0.0",
        "esbuild-runner": "^2.2.2",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.3.0",
        "eslint-plugin-promise": "^6.0.1",
        "jest": "^29.1.2",
        "safe-publish-latest": "^2.0.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "mysql2": "github:sidorares/node-mysql2#master"
    }
}
